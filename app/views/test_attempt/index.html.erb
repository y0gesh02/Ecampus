<% if @test_attempts.present? %>
  <h4>All Users</h4>
  <ul class="list-group list-group-light">
    <% @test_attempts.each do |t| %>
      <li class="list-group-item d-flex justify-content-between align-items-center">
        <div class="d-flex align-items-center">
          <div class="ms-3">
            <p class="fw-bold mb-1"><%= link_to t.user.name, t.user %></p>
          </div>
        </div>
        <div class="d-flex align-items-center">
          <div class="ms-3">
            <p class="fw-bold mb-1">Started at <%=t.started_at%></p>
            <p class="fw-bold mb-1">Submitted at <%=t.submitted_at%></p>
          </div>
        </div>
        <span class="badge rounded-pill badge-warning bg-primary text-dark">Total Score : <%= t.total_score %> </span>
      </li>
    <% end %>
  </ul>
<% else %>
  <h3>No User yet</h3>
<% end %>
